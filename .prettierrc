{
	"arrowParens": "always",
	"bracketSpacing": false,
	"printWidth": 80,
	"semi": false,
	"tabWidth": 2,
	"trailingComma": "all",
	"useTabs": true,
	"singleQuote": true,
	"overrides": [
		{
			"files": ["tsconfig*.json", ".vscode/*.json"],
			"options": {
				"parser": "json5",
				"quoteProps": "preserve",
				"singleQuote": false
			}
		}
	]
}
